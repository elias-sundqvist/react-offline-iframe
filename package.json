    "@typescript-eslint/parser": "^5.9.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "minify": "^8.0.3",
    "node-sass": "^5.0.0",
